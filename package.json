{
  "name": "@nammaplugin/autocomplete",
  "version": "1.2.0",
  "description": "Auto Complete Dropdown",
  "scripts": {
    "clean": "rimraf dist && rimraf coverage",
    "build": "npm run compile && npm run copy:html && npm run copy:scss && npm run bundle && npm run minify",
    "compile": "tsc",
    "copy:html": "cpx src/**.html dist",
    "copy:scss": "cpx src/**.scss dist",
    "test": "karma start",
    "bundle": "rollup -c",
    "minify": "uglifyjs dist/autocomplete-bundle.js --output dist/autocomplete-bundle.min.js"    
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/techaravi/AutoComplete.git"
  },
  "keywords": [
    "Auto Complete Dropdown",
    "Javascript",
    "TypeScript",
    "CSS",
    "SCSS"
  ],
  "author": "Aravinda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/techaravi/AutoComplete/issues"
  },
  "homepage": "https://github.com/techaravi/AutoComplete#readme",
  "devDependencies": {
    "@types/jasmine": "^3.8.2",
    "@types/jquery": "^3.5.6",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "cpx": "^1.5.0",
    "jasmine-core": "^3.8.0",
    "karma": "^6.3.4",
    "karma-jasmine": "^4.0.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-typescript": "^5.5.1",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.55.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5",
    "uglify-js": "^3.14.1"
  },
  "dependencies": {
    "jquery": "^3.6.0",
    "node-sass": "^6.0.1"
  },
  "main": "rollup.config.js"
}
